        <entry id="6d78ebca-1e16-3fcc-7210-efbbd98bbc74" name="Master Rune Of Flight" points="30.0" categoryId="(No Category)" type="upgrade" minSelections="0" maxSelections="1" minPoints="0.0" maxPoints="-1.0" minInRoster="0" maxInRoster="1" collective="false" page="0">
